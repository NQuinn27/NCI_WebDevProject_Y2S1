<packages>
    <!--
    **KEY FOR IMAGE URI**
    Teen Items
        to = topItem
        s = shirtItem
        t = trousersItem
        sh = shoes
        a = accessories

    Formal Items
        ja = jacketItem
        fs = fshirtItem (formal shirt)
        p = pantsItem
        fsh = fshoes (formal shoes)
        tie = tie

    URI Structure
    [item]_[month].jpg
    eg. to_jan.jpg = topItem for the month of January

     -->
    <package>
        <month>January</month>
        <style>Teen</style>
        <items>
            <clothingItem>
                <type>topItem</type>
                <description>Sweater</description>
                <imageUri>/img/to_jan.jpg</imageUri>
            </clothingItem>
            <clothingItem>
                <type>shirtItem</type>
                <description>Short-sleeved shirt</description>
                <imageUri>/img/s_jan.jpg</imageUri>
            <style></style>
            </clothingItem>
            <clothingItem>
                <type>trousersItem</type>
                <description>Denim trousars</description>
                <imageUri>/img/t_jan.jpg</imageUri>
            </clothingItem>
            <clothingItem>
                <type>shoes</type>
                <description>Converse</description>
                <imageUri>/img/sh_jan.jpg</imageUri>
            </clothingItem>
            <clothingItem>
                <type>accessories</type>
                <description>Beanie Hat</description>
                <imageUri>/img/a_jan.jpg</imageUri>
            </clothingItem>
        </items>
    </package>
